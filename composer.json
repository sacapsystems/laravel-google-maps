{
  "name": "sacapsystems/laravel-google-maps",
  "description": "Laravel Google Maps integration",
  "type": "library",
  "require": {
    "ext-json": "*",
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.10.0*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.13.4",
    "phpunit/phpunit": "^12.3.8",
    "orchestra/testbench": "^v10.6.0",
    "php-parallel-lint/php-parallel-lint": "^v1.4.0"
  },
  "autoload": {
    "psr-4": {
      "Sacapsystems\\LaravelGoogleMaps\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sacapsystems\\LaravelGoogleMaps\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "lint": "@php ./vendor/php-parallel-lint/php-parallel-lint/parallel-lint --exclude vendor --exclude node_modules --exclude dist --exclude .git --exclude .phpstan-cache .",
    "test": "@php -d pcov.enabled=1 -d pcov.directory=. -dpcov.exclude=\"~vendor~\" ./vendor/bin/phpunit --coverage-text --coverage-cobertura=coverage.xml",
    "cs-check": "@php ./vendor/bin/phpcs",
    "cs-fix": "@php ./vendor/bin/phpcbf"
  },
  "minimum-stability": "stable"
}
